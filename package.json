    "formik": "2.2.9",
    "graphql": "15.5.3",
    "inversify": "6.0.1",
    "next": "12.1.0",
    "next-i18next": "8.10.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "reflect-metadata": "0.1.13",
