{
  "name": "mrmilu.next.base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "start": "next start",
    "check-types": "tsc",
    "lint": "next lint --fix && yarn check-types",
    "prepare": "husky install",
    "commit": "commit",
    "graphql-schema": "apollo client:download-schema schema.json",
    "graphql-types": "apollo client:codegen --target=typescript --localSchemaFile=schema.json --globalTypesFile=src/__generated__/globalTypes.d.ts",
    "graphql-possible-types": "node generate_possible_types.js",
    "graphql": "yarn graphql-schema --endpoint=https://graphqlzero.almansi.me/api && yarn graphql-possible-types && yarn graphql-types"
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@reduxjs/toolkit": "1.8.0",
    "@sentry/nextjs": "6.18.2",
    "apollo": "2.33.9",
    "axios": "0.26.1",
    "class-transformer": "0.5.1",
    "formik": "2.2.9",
    "graphql": "15.5.3",
    "inversify": "6.0.1",
    "lodash": "4.17.21",
    "next": "12.1.0",
    "next-i18next": "10.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-spring": "9.4.4",
    "reflect-metadata": "0.1.13",
    "styled-components": "5.3.3",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@axe-core/react": "4.4.0",
    "@babel/core": "7.17.8",
    "@babel/plugin-proposal-decorators": "7.17.8",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@commitlint/prompt-cli": "16.2.3",
    "@next/bundle-analyzer": "12.1.0",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "@types/react-redux": "7.1.23",
    "@types/styled-components": "5.1.24",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "babel-plugin-import-graphql": "2.8.1",
    "babel-plugin-inline-react-svg": "2.0.1",
    "babel-plugin-styled-components": "2.0.6",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.4",
    "prettier": "2.6.0",
    "typescript": "4.6.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "resolutions": {
    "graphql": "15.5.3"
  }
}
