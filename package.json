{
  "name": "am.front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "commit": "commit",
    "graphql-schema": "apollo client:download-schema schema.graphql",
    "graphql-types": "apollo client:codegen --target=typescript --localSchemaFile=schema.graphql --globalTypesFile=src/__generated__/globalTypes.d.ts",
    "graphql": "yarn graphql-schema --endpoint=https://api.mocki.io/v2/c4d7a195/graphql/ && yarn graphql-types"
  },
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@reduxjs/toolkit": "1.6.2",
    "@sentry/nextjs": "6.13.3",
    "class-transformer": "^0.4.0",
    "formik": "2.2.9",
    "graphql": "15.5.3",
    "inversify": "6.0.1",
    "next": "12.0.2",
    "next-i18next": "8.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "reflect-metadata": "^0.1.13",
    "sass": "1.43.4"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-proposal-class-properties": "7.16.0",
    "@babel/plugin-proposal-decorators": "7.16.0",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@commitlint/prompt-cli": "13.2.1",
    "@types/node": "16.11.6",
    "@types/react": "17.0.32",
    "@types/react-redux": "7.1.20",
    "apollo": "2.33.7",
    "babel-plugin-import-graphql": "2.8.1",
    "babel-plugin-inline-react-svg": "2.0.1",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.2",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.0",
    "prettier": "2.4.1",
    "stylelint": "14.1.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "23.0.0",
    "stylelint-config-standard-scss": "2.0.1",
    "stylelint-selector-bem-pattern": "2.1.1",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "resolutions": {
    "graphql": "15.5.3"
  }
